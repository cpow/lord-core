<div class="row">
  <div class="col-12">
    <%= form_for @company do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.submit "Create Company", class: 'btn btn-primary' %>
      </div>
    <% end %>
  </div>
</div>
